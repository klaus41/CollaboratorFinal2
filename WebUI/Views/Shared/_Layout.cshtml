<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Collaborator - Elite IT</title>
    <!-- Bootstrap 3.3.6 -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link href="~/Content/lte/css/AdminLTE.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/lte/css/skins/skin-purple-light.css" rel="stylesheet" />

    <link href='~/Content/Custom.css?ver=<%= Application["version"] %>' rel="stylesheet" />
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js?ver=<%= Application['version'] %>"></script>
    <script src="http://canvasjs.com/assets/script/canvasjs.min.js?ver=<%= Application['version'] %>"></script>
    <script src="~/Scripts/jquery-3.1.0.js?ver=<%= Application['version'] %>"></script>
    <script src="~/Scripts/Custom.js?ver=<%= Application['version'] %>" type="text/javascript"></script>

</head>
<body class="hold-transition skin-purple-light sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <header class="main-header">

            @RenderPage("_Header.cshtml")

        </header>
        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    @*<div class="pull-left image">
                            <img src="../../dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                        </div>*@
                    <div class="pull-left info">
                        <p>Elite IT</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>

                @RenderPage("_Navigation.cshtml")
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            @*<section class="content-header">
                    <h1>
                        Fixed Layout
                        <small>Blank example to the fixed layout</small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                        <li><a href="#">Layout</a></li>
                        <li class="active">Fixed</li>
                    </ol>
                </section>*@
            <!-- Main content -->
            <section class="content">
                @*<div class="callout callout-info">
                        <h4>Tip!</h4>
                        <p>
                            Add the fixed class to the body tag to get this layout. The fixed layout is your best option if your sidebar
                            is bigger than your content because it prevents extra unwanted scrolling.
                        </p>
                    </div>*@
                <!-- Default box -->


                @RenderBody()

                <!-- /.box -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            @RenderPage("_Footer.cshtml")
        </footer>
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@

    <!-- jQuery 2.2.3 -->
    <script src="~/Content/lte/plugins/jQuery/jquery-2.2.3.min.js"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="~/Scripts/bootstrap.js"></script>
    <!-- SlimScroll -->
    <script src="~/Content/lte/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/lte/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/lte/js/app.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/lte/js/demo.js"></script>

</body>
</html>
